{
	"@timestamp": {
		"$resolver": "timestamp",
		"pattern": {
			"format": "yyyy-MM-dd HH:mm:ss.SSS",
			"timeZone": "UTC"
		}
	},
	"log.level": {
		"$resolver": "level",
		"field": "name"
	},
	"message": {
		"$resolver": "message",
		"stringified": true
	},
	"process.thread.name": {
		"$resolver": "thread",
		"field": "name"
	},
	"log.logger": {
		"$resolver": "logger",
		"field": "name"
	},
	"labels": {
		"$resolver": "mdc",
		"flatten": true,
		"stringified": true
	}
}
